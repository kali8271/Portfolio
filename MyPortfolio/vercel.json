{
    "builds": [
        {
            "src": "MyPortfolio/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "buildCommand": "bash build_files.sh",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "MyPortfolio/wsgi.py"
        }
    ]
}
